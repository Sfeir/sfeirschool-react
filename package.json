{
  "name": "react-people",
  "version": "0.0.1",
  "private": true,
  "description": "SfeirSchool React v2019",
  "main": "index.js",
  "scripts": {
    "start": "run-p watch:exos api serve",
    "prestart": "rm -rf .cache & rm -rf dist",
    "watch:exos": "parcel src/ex*/index.html src/favicon.ico",
