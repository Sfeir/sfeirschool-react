  },
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/standalone": "^7.2.5",
    "json-server": "^0.14.2",
    "less": "^3.9.0",
    "live-server": "^1.2.1",
    "normalize.css": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.11.0",
    "react": "next",
    "react-dom": "next",
    "rmwc": "^4.0.6"
  }
}
